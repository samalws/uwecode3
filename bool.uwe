{basics}
{nFuncs}
2 _= f -> x -> f (f x)
true = const
false = ignore
notBool = nRev 2 false true
andBool = a -> b -> a b false
orBool = a -> b -> a true b
xorBool = a -> b -> a (not b) b
nandBool = nComp 2 notBool andBool
norBool = nComp 2 notBool orBool
xnorBool = nComp 2 notBool xorBool
eqBool = xnorBool
