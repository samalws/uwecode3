{basics}
nComp2 _= x -> y -> f -> f x y
true = const
false = ignore
notBool = nRev2 false true
andBool = a -> b -> a b false
orBool = a -> b -> a true b
xorBool = a -> b -> a (not b) b
nandBool = nComp2 notBool andBool
norBool = nComp2 notBool orBool
xnorBool = nComp2 notBool xorBool
eqBool = xnorBool
